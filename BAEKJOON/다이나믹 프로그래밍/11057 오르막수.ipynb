   "source": [
    "n = int(input())\n",
    "\n",
    "dp = list([[1] * 10])\n",
    "\n",
    "for _ in range(n):\n",
    "    dp.append(list([0] * 10))\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    for j in range(0,10):\n",
    "        for k in range(j+1):\n",
    "            dp[i][j] += dp[i-1][k]\n",
    "\n",
    "print(dp[n][9] % 10007)"
   ]
  }
