   "source": [
    "n = int(input())\n",
    "\n",
    "li = [0]*(n+1)\n",
    "\n",
    "if n==0:\n",
    "    print(1)\n",
    "elif n==1:\n",
    "    print(0)\n",
    "elif n==2:\n",
    "    print(3)\n",
    "else:\n",
    "    li[0] = 1\n",
    "    li[2] = 3\n",
    "    for i in range(4,n+1,2):\n",
    "        li[i] = li[i-2]*3\n",
    "        for j in range(4,i+1,2):\n",
    "            li[i] += li[i-j]*2\n",
    "    print(li[n])"
   ]
